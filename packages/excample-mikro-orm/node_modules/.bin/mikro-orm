#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xmtj/Desktop/学习/Monorepo/node_modules/.pnpm/@mikro-orm+cli@5.9.6_@mikro-orm+entity-generator@5.9.6_@mikro-orm+migrations@5.9.6_@mikro-orm_6yqxgexkpasx2rqsqj7atkhj4y/node_modules/@mikro-orm/cli/node_modules:/Users/xmtj/Desktop/学习/Monorepo/node_modules/.pnpm/@mikro-orm+cli@5.9.6_@mikro-orm+entity-generator@5.9.6_@mikro-orm+migrations@5.9.6_@mikro-orm_6yqxgexkpasx2rqsqj7atkhj4y/node_modules/@mikro-orm/node_modules:/Users/xmtj/Desktop/学习/Monorepo/node_modules/.pnpm/@mikro-orm+cli@5.9.6_@mikro-orm+entity-generator@5.9.6_@mikro-orm+migrations@5.9.6_@mikro-orm_6yqxgexkpasx2rqsqj7atkhj4y/node_modules:/Users/xmtj/Desktop/学习/Monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xmtj/Desktop/学习/Monorepo/node_modules/.pnpm/@mikro-orm+cli@5.9.6_@mikro-orm+entity-generator@5.9.6_@mikro-orm+migrations@5.9.6_@mikro-orm_6yqxgexkpasx2rqsqj7atkhj4y/node_modules/@mikro-orm/cli/node_modules:/Users/xmtj/Desktop/学习/Monorepo/node_modules/.pnpm/@mikro-orm+cli@5.9.6_@mikro-orm+entity-generator@5.9.6_@mikro-orm+migrations@5.9.6_@mikro-orm_6yqxgexkpasx2rqsqj7atkhj4y/node_modules/@mikro-orm/node_modules:/Users/xmtj/Desktop/学习/Monorepo/node_modules/.pnpm/@mikro-orm+cli@5.9.6_@mikro-orm+entity-generator@5.9.6_@mikro-orm+migrations@5.9.6_@mikro-orm_6yqxgexkpasx2rqsqj7atkhj4y/node_modules:/Users/xmtj/Desktop/学习/Monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@mikro-orm/cli/cli.js" "$@"
else
  exec node  "$basedir/../@mikro-orm/cli/cli.js" "$@"
fi
